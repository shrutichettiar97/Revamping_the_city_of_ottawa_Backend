l":{"line":92,"column":8}},{"generated":{"line":173,"column":15},"source":"components/Root/Login/Login.js","original":{"line":90,"column":6}},{"generated":{"line":173,"column":16},"source":"components/Root/Login/Login.js","original":{"line":68,"column":4}},{"generated":{"line":173,"column":17},"source":"components/Root/Login/Login.js","original":{"line":67,"column":2}},{"generated":{"line":174,"column":0},"source":"components/Root/Login/Login.js","original":{"line":104,"column":1}},{"generated":{"line":174,"column":1},"source":"components/Root/Login/Login.js","original":{"line":45,"column":0}},{"name":"Login","generated":{"line":176,"column":15},"source":"components/Root/Login/Login.js","original":{"line":106,"column":15}}],"sources":{"components/Root/Login/Login.js":"import React from \"react\";\r\nimport { useForm } from \"react-hook-form\";\r\nimport styled from \"styled-components\";\r\nimport TextInput from \"#root/components/shared/TextInput\";\r\nimport { useMutation } from \"@apollo/react-hooks\";\r\nimport gql from \"graphql-tag\";\r\nimport { setSession } from \"#root/store/ducks/session\";\r\nimport { useDispatch } from \"react-redux\";\r\n\r\nconst Label = styled.label`\r\n  display: block;\r\n\r\n  :not(:frost-child) {\r\n    margin-top: 0.75rem;\r\n  }\r\n`;\r\n\r\nconst LabelText = styled.strong`\r\n  display: block;\r\n  font-size: 1rem;\r\n  margin-bottom: 0.25rem;\r\n`;\r\n\r\nconst LoginButton = styled.button`\r\n  disolay: inline-block;\r\n  margin-top: 0.5rem;\r\n`;\r\n\r\nconst OrSignUp = styled.span`\r\n  font-size: 0.8rem;\r\n`;\r\n\r\nconst mutation = gql`\r\n  mutation($email: String!, $password: String!) {\r\n    createUserSession(email: $email, password: $password) {\r\n      id\r\n      user {\r\n        email\r\n        id\r\n      }\r\n    }\r\n  }\r\n`;\r\n\r\nconst Login = ({ onChangeToSignUp: pushChangeToSignUp }) => {\r\n  const dispatch = useDispatch();\r\n  const [createUserSession] = useMutation(mutation);\r\n  const {\r\n    formState: { isSubmitting },\r\n    handleSubmit,\r\n    register,\r\n  } = useForm();\r\n\r\n  const onSubmit = handleSubmit(async ({ email, password }) => {\r\n    const {\r\n      data: { createUserSession: createdSession },\r\n    } = await createUserSession({\r\n      variables: {\r\n        email,\r\n        password,\r\n      },\r\n    });\r\n    console.log(\"createdSession\", createdSession);\r\n    dispatch(setSession(createdSession));\r\n  });\r\n\r\n  return (\r\n    <form onSubmit={onSubmit}>\r\n      <Label>\r\n        <LabelText>Email</LabelText>\r\n        <TextInput\r\n          disabled={isSubmitting}\r\n          name=\"email\"\r\n          type=\"email\"\r\n          ref={register}\r\n        />\r\n      </Label>\r\n      <Label>\r\n        <LabelText>Password</LabelText>\r\n        <TextInput\r\n          disabled={isSubmitting}\r\n          name=\"password\"\r\n          type=\"password\"\r\n          ref={register}\r\n        />\r\n      </Label>\r\n      <LoginButton disabled={isSubmitting} tyle=\"submit\">\r\n        Login\r\n      </LoginButton>\r\n      <OrSignUp>\r\n        or{\" \"}\r\n        <a\r\n          href=\"#\"\r\n          onClick={(evt) => {\r\n            evt.preventDefault();\r\n            pushChangeToSignUp();\r\n          }}\r\n        >\r\n          Sign up\r\n        </a>\r\n      </OrSignUp>\r\n    </form>\r\n  );\r\n};\r\n\r\nexport default Login;\r\n"},"lineCount":null}},"error":null,"hash":"817c114a782be7b0f43f7fa86d0e6951","cacheData":{"env":{}}}